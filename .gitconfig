[user]
	name = Alex Kahn
	email = alex.kahn@netlify.com
	signingkey = ssh-rsa 
[core]
	excludesfile = /Users/akahn/.gitignore
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[merge]
	log = true
[push]
	default=current
[github]
	user = akahn
	token = 9c38bca2a2b071073fc160adfb1a87f841f3de16
[pull]
	rebase = false
[alias]
	br = branch --sort=-committerdate
	co = checkout
	sha = !"git rev-parse HEAD | tee /dev/tty | tr -d '\n' | pbcopy"
  last = !"git show HEAD --pretty="%H" --no-patch"
[init]
	defaultBranch = main
[url "git@github.com:"]
	insteadOf = https://github.com/
[gpg]
	format = ssh
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
[commit]
	gpgsign = true
